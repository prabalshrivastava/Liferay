		<property name="message" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="fileEntryId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="associatedWith" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="restricted" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="parentConverstationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="currentPlId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.sambaash.platform.srv.model.impl.ConversationUserImpl" table="SPConversationUser">
		<cache usage="read-write" />
		<id name="spConversationUserId" type="long">
			<generator class="assigned" />
		</id>
		<property name="spConversationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="sentToUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.LongType" />
