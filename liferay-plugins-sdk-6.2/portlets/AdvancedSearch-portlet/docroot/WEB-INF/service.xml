<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.sambaash.platform.srv.filter">
	<author>harini</author>
	<namespace>SPSearchFilter</namespace>

	<entity name="SPSearchFilter" table="SPSearchFilter" uuid="true" local-service="true"
		remote-service="false">


		<column name="spSearchFilterId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<column name="filterName" type="String" />
		<column name="filterParameter" type="String" />


		<finder name="userId" return-type="Collection">
			<finder-column name="userId"></finder-column>
		</finder>

	</entity>

</service-builder>