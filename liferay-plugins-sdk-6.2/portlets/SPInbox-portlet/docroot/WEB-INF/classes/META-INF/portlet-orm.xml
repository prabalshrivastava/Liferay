<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.sambaash.platform.srv.spinbox.model.impl.IBMessageModelImpl">
		<attributes>
			<id name="messageId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="parentMessageId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="subject" />
			<basic name="content" />
			<basic name="from">
				<column name="FROM_" />
			</basic>
			<basic name="to">
				<column name="TO_" />
			</basic>
			<basic name="allowOpen" />
			<basic name="sendDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="createBy" />
			<basic name="createByUserId" />
			<basic name="draft" />
			<basic name="deleteStatus" />
			<basic name="draftStatus" />
			<basic name="sentMeCopy" />
			<basic name="belongToGroupDetailId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.sambaash.platform.srv.spinbox.model.impl.IBMessageDetailModelImpl">
		<attributes>
			<id name="ibMsgDetailId" />
			<basic name="receiverId" />
			<basic name="messageId" />
			<basic name="receiverMsgStatus" />
			<basic name="senderMsgStatus" />
			<basic name="status" />
			<basic name="receiveDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="receiveBy" />
			<basic name="archived" />
			<basic name="updateDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="updateBy" />
			<basic name="category" />
			<basic name="processId" />
			<basic name="corpProfileId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.sambaash.platform.srv.spinbox.model.impl.IBMessageImpl" name="IBMessage">
		<table name="SPInboxMessage" />
		<attributes />
	</entity>
	<entity class="com.sambaash.platform.srv.spinbox.model.impl.IBMessageDetailImpl" name="IBMessageDetail">
		<table name="SPInboxMessageDetail" />
		<attributes />
	</entity>
</entity-mappings>