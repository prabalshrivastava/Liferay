<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">

	<portlet>
		<portlet-name>SPAssetType</portlet-name>
		<display-name>SPAssetType</display-name>
		<portlet-class>
			com.sambaash.platform.portlet.spasset.action.SPAssetTypeAction
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/spassettype/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>SPAssetType</title>
			<short-title>SPAssetType</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>SPAssetEntry</portlet-name>
		<display-name>SPAssetEntry</display-name>
		<portlet-class>
			com.sambaash.platform.portlet.spasset.action.SPAssetEntryAction
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/spassetentry/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/spassetentry/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>SPAssetEntry</title>
			<short-title>SPAssetEntry</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
	<portlet>
		<portlet-name>spassetdetailview</portlet-name>
		<display-name>Sp Asset Detail View</display-name>
		<portlet-class>
			com.sambaash.platform.portlet.spasset.action.SPAssetDetailView
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/detail/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/detail/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Sp Asset Detail View</title>
			<short-title>Spassetdetailview</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>
