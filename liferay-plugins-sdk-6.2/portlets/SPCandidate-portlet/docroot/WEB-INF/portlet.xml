			com.sambaash.platform.candidate.CandidatePortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/candidate/edit.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/candidate/config.jsp</value>
		</init-param>
		
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>config</portlet-mode>
			<portlet-mode>edit_defaults</portlet-mode>
		</supports>
		<portlet-info>
			<title>Candidate</title>
			<short-title>Candidate</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
