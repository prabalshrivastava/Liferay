		<finder name="CountryIdAndGroupId" return-type="Collection">
			<finder-column name="countryId" />
			<finder-column name="groupId" />
		</finder>
		
		<finder name="NameAndGroupId" return-type="Framework">
			<finder-column name="frameworkName" case-sensitive="false"/>
