<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="2.0">
	<portlet>
		<portlet-name>SPATOAdmission</portlet-name>
		<display-name>SPATOAdmission</display-name>
		<portlet-class>com.liferay.util.bridges.mvc.MVCPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/atoadmission/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>config</portlet-mode>

		</supports>

		<portlet-info>
			<title>SPATOAdmission</title>
			<short-title>SPATOAdmission</short-title>
			<keywords>SPATOAdmission</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>ATO-Adminssion</portlet-name>
		<display-name>ATO Adminssion</display-name>
		<portlet-class>com.sambaash.platform.ato.ATOAdmission</portlet-class>
		<init-param>
			<name>details-template</name>
			<value>/html/atoadmission/view.jsp</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/html/atoadmission/list.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/atoadmission/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>config</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>ATO Adminssion</title>
			<short-title>ATO Adminssion</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>ATO-Sponsered-Candidates</portlet-name>
		<display-name>ATO Sponsered Candidates</display-name>
		<portlet-class>
			com.sambaash.platform.ato.sponseredcandidates.SponseredCandidates</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/sponsered-candidates/custom-listing.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/sponsered-candidates/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>config</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Sponsered Candidates</title>
			<short-title>Sponsered Candidates</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>