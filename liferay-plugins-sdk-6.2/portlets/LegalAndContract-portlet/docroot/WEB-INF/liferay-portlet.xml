<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>CreateAgency</portlet-name>
		<icon>/icon.png</icon>
		<indexer-class>com.sambaash.platform.portlet.legalandcontract.util.AgencyIndexer</indexer-class>
		<open-search-class>com.sambaash.platform.portlet.legalandcontract.util.AgencyOpenSearchImpl</open-search-class>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>agency</friendly-url-mapping>
		<friendly-url-routes>com/sambaash/platform/portlet/legalandcontract/routes/agencies-details-routes.xml</friendly-url-routes>
		<asset-renderer-factory>com.sambaash.platform.portlet.legalandcontract.util.AgencyRendererFactory</asset-renderer-factory>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>
			/js/main.js
		</header-portlet-javascript>
		<css-class-wrapper>createagency-portlet</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>SPClass</portlet-name>
		<icon>/icon.png</icon>
		<indexer-class>com.sambaash.platform.portlet.legalandcontract.util.ClassMasterIndexer</indexer-class>
		<open-search-class>com.sambaash.platform.portlet.legalandcontract.util.ClassMasterOpenSearchImpl</open-search-class>
		<asset-renderer-factory>com.sambaash.platform.portlet.legalandcontract.util.ClassMasterRendererFactory</asset-renderer-factory>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>
			/js/main.js
		</header-portlet-javascript>
		<css-class-wrapper>spclass-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>trade-marks-master</portlet-name>
		<icon>/icon.png</icon>
		<indexer-class>com.sambaash.platform.portlet.legalandcontract.util.TrademarksIndexer</indexer-class>
		<open-search-class>com.sambaash.platform.portlet.legalandcontract.util.TrademarksOpenSearchImpl</open-search-class>
		<scheduler-entry>
			<scheduler-description>RenewalAlert</scheduler-description>
			<scheduler-event-listener-class>com.sambaash.platform.portlet.legalandcontract.schedular.RenewalAlertSchedular</scheduler-event-listener-class>
			<trigger>
				<cron>
					<cron-trigger-value>0 30 9 * * ?</cron-trigger-value>
				</cron>
			</trigger>
		</scheduler-entry>

		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>trademarks</friendly-url-mapping>
		<friendly-url-routes>com/sambaash/platform/portlet/legalandcontract/routes/trademarks-details-routes.xml</friendly-url-routes>
		<asset-renderer-factory>com.sambaash.platform.portlet.legalandcontract.util.TrademarkRendererFactory</asset-renderer-factory>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>
			/js/main.js
		</header-portlet-javascript>
		<css-class-wrapper>
			trade-marks-master-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>litigation-action</portlet-name>
		<icon>/icon.png</icon>
		<indexer-class>com.sambaash.platform.portlet.legalandcontract.util.LitigationIndexer</indexer-class>
		<indexer-class>com.sambaash.platform.portlet.legalandcontract.util.RDLIndexer</indexer-class>
		<open-search-class>com.sambaash.platform.portlet.legalandcontract.util.LitigationOpenSearchImpl</open-search-class>
		<scheduler-entry>
			<scheduler-description>RenewalAlert</scheduler-description>
			<scheduler-event-listener-class>com.sambaash.platform.portlet.legalandcontract.schedular.RDLSchedular</scheduler-event-listener-class>
			<trigger>
				<cron>
					<cron-trigger-value>0 45 9 * * ?</cron-trigger-value>
				</cron>
			</trigger>
		</scheduler-entry>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>litigation</friendly-url-mapping>
		<friendly-url-routes>com/sambaash/platform/portlet/legalandcontract/routes/litigations-details-routes.xml</friendly-url-routes>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>
			/js/main.js
		</header-portlet-javascript>
		<css-class-wrapper>litigation-action-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>