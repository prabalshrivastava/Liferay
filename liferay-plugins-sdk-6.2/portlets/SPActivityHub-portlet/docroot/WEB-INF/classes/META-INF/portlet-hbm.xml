		<property name="activityTitle" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="activityType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="activityOutcome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="activityDate" type="org.hibernate.type.TimestampType" />
		<property name="activityTime" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="activityContent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="fileEntryId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="associatedWith" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="parentLogActivityId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.sambaash.platform.srv.model.impl.NoteImpl" table="SPNote">
		<cache usage="read-write" />
		<id name="spNoteId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="entityClassId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="entityClassName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="entityId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="savedByUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="noteTitle" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="noteContent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="fileEntryId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="associatedWith" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="parentNoteId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
