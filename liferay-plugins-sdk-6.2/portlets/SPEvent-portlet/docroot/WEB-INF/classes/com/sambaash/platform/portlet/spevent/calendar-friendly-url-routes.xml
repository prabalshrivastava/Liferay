<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">
 <routes>
    <route>
		<pattern>/{eventId}</pattern>
		<generated-parameter name="jspPage">/html/spevent/view.jsp</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="struts_action">/html/spevent/view</implicit-parameter>
		<implicit-parameter name="eventId">{eventId}</implicit-parameter>
	</route>
	
	<route>
		<pattern>/{month}/{day}/{year}</pattern>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="struts_action">/calendar/edit_event</implicit-parameter>
	</route>
</routes>

