<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>sp-periscope-data</portlet-name>
		<display-name>Sp Periscope Data</display-name>
		<portlet-class>
			com.sambaash.platform.spperiscopedata.action.SPPeriscopeDataAction
		</portlet-class>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/html/spperiscopedata/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/spperiscopedata/edit.jsp</value>
		</init-param>
<!-- 		<init-param> -->
<!-- 			<name>config-template</name> -->
<!-- 			<value>/html/spperiscopedata/config.jsp</value> -->
<!-- 		</init-param> -->
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
<!-- 			<portlet-mode>config</portlet-mode> -->
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Sp Periscope Data</title>
			<short-title>Sp Periscope Data</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>