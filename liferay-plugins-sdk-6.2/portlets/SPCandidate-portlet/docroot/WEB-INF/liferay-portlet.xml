		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>candidate-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>candidate-list</portlet-name>
		<icon>/icon.png</icon>
