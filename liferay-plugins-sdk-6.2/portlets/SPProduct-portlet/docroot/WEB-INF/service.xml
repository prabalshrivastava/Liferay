	<entity name="Framework" table="SPFramework" local-service="true"
		remote-service="false">

		<!-- PK fields -->

		<column name="spFrameworkId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
