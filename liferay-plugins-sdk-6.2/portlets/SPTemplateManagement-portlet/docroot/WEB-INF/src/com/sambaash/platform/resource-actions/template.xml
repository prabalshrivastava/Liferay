<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 6.2.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_6_2_0.dtd">

<resource-action-mapping>
    <portlet-resource>
        <portlet-name>template</portlet-name>
        <permissions>
            <supports>
                <action-key>VIEW_TEMPLATES</action-key>
                <action-key>VIEW_TEMPLATE</action-key>
                <action-key>ADD_TEMPLATE</action-key>
                <action-key>UPDATE_TEMPLATE</action-key>
                <action-key>DELETE_TEMPLATE</action-key>
                <action-key>ADD_PORTLET_DISPLAY_TEMPLATE</action-key>
            </supports>
            <site-member-defaults>
            	<action-key>VIEW_TEMPLATES</action-key>
                <action-key>VIEW_TEMPLATE</action-key>
                <action-key>ADD_TEMPLATE</action-key>
                <action-key>UPDATE_TEMPLATE</action-key>
                <action-key>DELETE_TEMPLATE</action-key>
                <action-key>ADD_PORTLET_DISPLAY_TEMPLATE</action-key>
            </site-member-defaults>
            <guest-defaults>
            	
            </guest-defaults>
            
        </permissions>
    </portlet-resource>
</resource-action-mapping>