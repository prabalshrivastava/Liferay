<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>spinboxnew</portlet-name>
		<display-name>Inbox New</display-name>
		<portlet-class>
			com.sambaash.platform.portlet.spinbox.action.InboxAction
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/inbox/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/inbox/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>SP Inbox New</title>
			<short-title>SP Inbox New</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>spinboxlink</portlet-name>
		<display-name>SP Inbox Links</display-name>
		<portlet-class>
			com.sambaash.platform.portlet.spinbox.action.InboxLinksAction
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/inboxlinks/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/inboxlinks/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>SP Inbox Links</title>
			<short-title>Spinboxlink</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>