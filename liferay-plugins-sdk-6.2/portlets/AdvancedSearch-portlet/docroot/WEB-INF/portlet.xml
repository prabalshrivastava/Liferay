<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>AdvancedSearch</portlet-name>
		<display-name>AdvancedSearch</display-name>
		<portlet-class>com.sambaash.platform.portlet.advancedsearch.action.AdvancedSearchAction</portlet-class>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/advancedsearch/edit.jsp</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/html/advancedsearch/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>AdvancedSearch</title>
			<short-title>AdvancedSearch</short-title>
			<keywords>AdvancedSearch</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>DynamicLandingPageFeeds</portlet-name>
		<display-name>DynamicLandingPageFeeds</display-name>
		<portlet-class>com.sambaash.platform.portlet.advancedsearch.action.DynamicLandingPageFeedsAction</portlet-class>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/dynamiclandingpagefeeds/edit.jsp</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/html/dynamiclandingpagefeeds/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>DynamicLandingPageFeeds</title>
			<short-title>DynamicLandingPageFeeds</short-title>
			<keywords>DynamicLandingPageFeeds</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>