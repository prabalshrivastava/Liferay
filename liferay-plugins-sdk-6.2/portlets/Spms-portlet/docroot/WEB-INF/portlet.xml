<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>MembershipSubscription</portlet-name>
		<display-name>MembershipSubscription</display-name>
		<portlet-class>com.sambaash.platform.portlet.spms.action.MembershipSubscriptionAction</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/membershipsubscription/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>MembershipSubscription</title>
			<short-title>MembershipSubscription</short-title>
			<keywords>MembershipSubscription</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>MembershipUpgradeSubscription</portlet-name>
		<display-name>MembershipUpgradeSubscription</display-name>
		<portlet-class>com.sambaash.platform.portlet.upgrade.action.MembershipUpgradeSubscriptionAction</portlet-class>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/membershipupgradesubscription/edit.jsp</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/html/membershipupgradesubscription/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>MembershipUpgradeSubscription</title>
			<short-title>MembershipUpgradeSubscription</short-title>
			<keywords>MembershipUpgradeSubscription</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>